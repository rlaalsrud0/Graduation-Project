<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MypagePasswd">

    <ImageView
        android:id="@+id/mypage"
        android:layout_width="110dp"
        android:layout_height="44dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.388"
        app:layout_constraintStart_toEndOf="@+id/back"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.059"
        app:srcCompat="@drawable/mypage" />

    <Button
        android:id="@+id/back"
        android:layout_width="23dp"
        android:layout_height="23dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="55dp"
        android:background="@drawable/back"
        android:text=""
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/v_mypage_passwd_text"
        android:layout_width="345dp"
        android:layout_height="44dp"
        android:layout_marginTop="219dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.469"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/v_mypage_passwd_text" />

    <ImageView
        android:id="@+id/v_mypage_passwd_textinput"
        android:layout_width="346dp"
        android:layout_height="44dp"
        android:layout_marginTop="354dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/v_mypage_passwd_textinput" />

    <Button
        android:id="@+id/find_passwd"
        android:layout_width="103dp"
        android:layout_height="26dp"
        android:background="@drawable/v_mypage_passwd_seaarch_btn"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.896"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/v_mypage_passwd_textinput"
        app:layout_constraintVertical_bias="0.025" />

    <Button
        android:id="@+id/input"
        android:layout_width="wrap_content"
        android:layout_height="122dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/input"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/passwd"
        android:layout_width="242dp"
        android:layout_height="39dp"
        android:background="@null"
        android:ems="10"
        android:hint="비밀번호를 입력하세요."
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="@+id/v_mypage_passwd_textinput"
        app:layout_constraintEnd_toEndOf="@+id/v_mypage_passwd_textinput"
        app:layout_constraintHorizontal_bias="0.913"
        app:layout_constraintStart_toStartOf="@+id/v_mypage_passwd_textinput"
        app:layout_constraintTop_toTopOf="@+id/v_mypage_passwd_textinput"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>