<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PMenuSelectTime">

    <ImageView
        android:id="@+id/menu_select_out"
        android:layout_width="110dp"
        android:layout_height="44dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.388"
        app:layout_constraintStart_toEndOf="@+id/back"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.059"
        app:srcCompat="@drawable/menu_select_time_textview" />

    <ImageView
        android:id="@+id/menu_select_box"
        android:layout_width="375dp"
        android:layout_height="54dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/menu_select_textview"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.361"
        app:srcCompat="@drawable/menu_select_box" />

    <ImageView
        android:id="@+id/menu_select_textview"
        android:layout_width="74dp"
        android:layout_height="29dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="216dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/menu_select_textview" />

    <Spinner
        android:id="@+id/vspinner"
        android:layout_width="346dp"
        android:layout_height="46dp"
        app:layout_constraintBottom_toBottomOf="@+id/menu_select_box"
        app:layout_constraintEnd_toEndOf="@+id/menu_select_box"
        app:layout_constraintHorizontal_bias="0.275"
        app:layout_constraintStart_toStartOf="@+id/menu_select_box"
        app:layout_constraintTop_toBottomOf="@+id/menu_select_textview"
        app:layout_constraintVertical_bias="0.859"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/plus"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="52dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/home_plus"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/home"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="107dp"
        android:layout_marginBottom="25dp"
        android:background="@drawable/home_home"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/plus"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/menu"
        android:layout_width="30dp"
        android:layout_height="20dp"
        android:layout_marginEnd="66dp"
        android:layout_marginBottom="23dp"
        android:background="@drawable/home_nav"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/select"
        android:layout_width="414dp"
        android:layout_height="132dp"
        android:layout_marginTop="414dp"
        android:background="@drawable/menu_select_btn"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/back"
        android:layout_width="23dp"
        android:layout_height="23dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="55dp"
        android:background="@drawable/back"
        android:text=""
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/drobdownlist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:constraint_referenced_ids="menu_select_box,menu_select_textview" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/navigationbar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:constraint_referenced_ids="plus,home,menu" />

</androidx.constraintlayout.widget.ConstraintLayout>